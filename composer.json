{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=7.4.0",
        "ext-json": "*",
        "ext-pdo_mysql": "*",
        "bravecollective/web-ui": "*@dev",
        "eveseat/eseye": "^2.0.0",
        "fideloper/proxy": "^4.0",
        "frontpack/composer-assets-plugin": "^0.11.0",
        "ixudra/curl": "^6.13",
        "jdavidbakr/replaceable-model": "^1.0",
        "laravel/framework": "^7.0",
        "laravel/helpers": "^1.6",
        "laravel/socialite": "^5.0",
        "laravel/tinker": "^2.0"
    },
    "require-dev": {
        "barryvdh/laravel-ide-helper": "^2.5",
        "doctrine/dbal": "^2.6",
        "driftingly/rector-laravel": "^0.24.0",
        "filp/whoops": "^2.0",
        "fzaninotto/faker": "^1.4",
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "^8.5",
        "rector/rector": "^0.18.0"
    },
    "repositories": [
        { "type": "git", "url": "https://github.com/bravecollective/web-ui.git" }
    ],
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [
            ]
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ]
    },
    "config": {
        "assets-dir": "public/assets",
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true,
        "platform": {
            
        },
        "allow-plugins": {
            "kylekatarnls/update-helper": true,
            "frontpack/composer-assets-plugin": true
        }
    }
}
